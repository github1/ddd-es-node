{
  "name": "ddd-es-node",
  "version": "0.4.3",
  "description": "DDD and Event Sourcing primitives for nodejs",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.9.0"
  },
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "tslint -t stylish --exclude '**/*.js' --project 'tsconfig.json'",
    "pretest": "npm run lint && eslint .",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run clean && npm run build && ./prepare.sh"
  },
  "dependencies": {
    "eventstore": "1.15.5",
    "reflect-metadata": "0.1.13",
    "uuid": "8.3.0",
    "walk": "2.3.9"
  },
  "devDependencies": {
    "@types/jest": "26.0.9",
    "@types/node": "12.12.6",
    "@types/uuid": "8.0.1",
    "eslint": "5.10.0",
    "eslint-plugin-jest": "20.0.3",
    "jest": "26.2.2",
    "rimraf": "2.6.2",
    "ts-jest": "26.1.4",
    "tslint": "6.1.3",
    "tslint-microsoft-contrib": "6.0.0",
    "tsutils": "3.17.1",
    "typescript": "4.3.5"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": "John Collier <john.collier4@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/github1/ddd-es-node"
  }
}
